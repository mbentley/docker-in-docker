{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "ingress-nginx",
    "namespace": "ingress-nginx"
  },
  "spec": {
    "type": "NodePort",
    "ports": [
      {
        "name": "http",
        "port": 80,
        "targetPort": 80,
        "nodePort": 34080,
        "protocol": "TCP"
      },
      {
        "name": "https",
        "port": 443,
        "targetPort": 443,
        "nodePort": 34443,
        "protocol": "TCP"
      }
    ],
    "selector": {
      "app": "ingress-nginx"
    }
  }
}